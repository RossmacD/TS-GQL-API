{
  "name": "apollo-typescript-starter",
  "version": "1.0.0",
  "description": "Apollo Server, GraphQL, Typescript and Postgres",
  "author": "Ross MacDonald",
  "repository": {
    "type": "git",
    "url": "git://github.com/kylealwyn/apollo-typescript-starter.git"
  },
  "keywords": [
    "node",
    "express",
    "graphql",
    "typescript",
    "apollo",
    "apollo-server",
    "postgres"
  ],
  "scripts": {
    "start": "yarn dev",
    "dev": "ts-node-dev --respawn ./src/main.ts --no-deps",
    "debug": "nodemon",
    "!up": "knex migrate:latest",
    "!down": "knex migrate:rollback"
  },
  "dependencies": {
    "apollo-server": "^2.1.0",
    "apollo-server-express": "^2.1.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.18.3",
    "compression": "^1.7.4",
    "connect-redis": "^5.0.0",
    "core-js": "^3.6.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.3",
    "express-session": "^1.17.1",
    "graphql": "^15.3.0",
    "helmet": "^3.23.3",
    "ioredis": "^4.17.3",
    "knex": "^0.21.2",
    "morgan": "^1.9.1",
    "pg": "^8.3.0",
    "reflect-metadata": "^0.1.13",
    "ts-node-dev": "^1.0.0-pre.51",
    "type-graphql": "^1.0.0-rc.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/body-parser": "^1.17.0",
    "@types/compression": "^1.7.0",
    "@types/connect-redis": "^0.0.14",
    "@types/express": "^4.16.0",
    "@types/express-session": "^1.17.0",
    "@types/helmet": "0.0.47",
    "@types/ioredis": "^4.17.2",
    "@types/knex": "^0.16.1",
    "@types/lodash": "^4.14.117",
    "@types/morgan": "^1.7.35",
    "@types/node": "^14.0.23",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.15.0",
    "tslint-plugin-prettier": "^2.0.0",
    "typescript": "^3.1.3"
  }
}
